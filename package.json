{
  "name": "@proux/www",
  "description": "proux website",
  "private": true,
  "scripts": {
    "prebuild": "real-favicon generate faviconDescription.json faviconData.json static/assets",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "now-build": "npm run build && next export -o dist",
    "test": "stylelint style.css && standard --fix && csslint --ignore=box-sizing style.css",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect",
    "a11y": "pa11y http://localhost:3000"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/proux/www.git"
  },
  "author": "Nico Lindemann <nico.lindemann@proux.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/proux/www/issues"
  },
  "homepage": "https://github.com/proux/www#readme",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.18",
    "@fortawesome/free-brands-svg-icons": "^5.8.2",
    "@fortawesome/free-solid-svg-icons": "^5.8.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@proux/logo": "git+https://git@github.com/proux/logo.git",
    "@zeit/next-css": "^1.0.1",
    "babel-plugin-inline-react-svg": "^1.1.0",
    "cli-real-favicon": "0.0.7",
    "get-google-fonts": "^1.1.3",
    "next": "^8.1.0",
    "next-offline": "^3.4.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      "inline-react-svg"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "comment-empty-line-before": null
    }
  },
  "snyk": true,
  "devDependencies": {
    "stylelint": "^10.0.1",
    "pa11y": "^5.1.0",
    "standard": "^12.0.1",
    "stylelint-config-standard": "^18.3.0",
    "snyk": "^1.163.3",
    "csslint": "^1.0.5",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-jsx": "^6.0.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-standard": "^4.0.0"
  }
}
